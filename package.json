{
  "name": "health-care-backend",
  "version": "1.0.0",
  "description": "Backend API para aplicação de gerenciamento de saúde familiar",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "seed": "tsx scripts/seed.ts",
    "migrate": "tsx scripts/migrate.ts",
    "generate:vapid": "tsx scripts/generate-vapid.ts",
    "health": "tsx scripts/check-health.ts",
    "db:reset": "tsx scripts/reset-db.ts",
    "setup": "bash scripts/setup.sh",
    "docker:build": "docker build -t health-care-api .",
    "docker:run": "docker run -p 3000:3000 --env-file .env health-care-api"
  },
  "keywords": [
    "health",
    "elderly",
    "medication",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.79.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "zod": "^3.22.4",
    "date-fns": "^3.6.0",
    "node-cron": "^3.0.3",
    "web-push": "^3.6.6",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.33.0",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/cors": "^2.8.17",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node-cron": "^3.0.11",
    "@types/web-push": "^3.6.3",
    "typescript": "^5.3.3",
    "tsx": "^4.7.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
